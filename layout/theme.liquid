  })(window,document,'script','dataLayer','GTM-KL8CFJG');</script>
  <!-- End Google Tag Manager -->
  
  <!-- fonts -->
  {% render 'head_fonts' %}
  
  <!-- Styles -->
  {% render 'head_styles' %}
  
   <!-- Scripts -->
  {% render 'head_scripts' %}

  <!-- Header hook for plugins ================================ -->
  {{ content_for_header }}
  {% comment %} <!-- Deprecated for IE --> {% endcomment %}
  <!--[if lt IE 9]>
  {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
